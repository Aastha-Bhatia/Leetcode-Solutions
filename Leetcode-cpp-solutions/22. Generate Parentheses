class Solution {
public:
    void generate(vector<string> &ans,int oc,int cc,int n,string s){

        if(oc == n && cc == n){

            ans.push_back(s);
            return ;
        }

        if(oc < n){

            generate(ans,oc+1,cc,n,s + '(');
        }

        if(cc < oc){
            generate(ans,oc,cc+1,n,s+')');
        }
    }

    vector<string> generateParenthesis(int n) {
        
        vector<string> ans;

        int oc = 0;
        int cc = 0;

        string s = "";

        generate(ans,oc,cc,n,s);

        return ans;
    }
};