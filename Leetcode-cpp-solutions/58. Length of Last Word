class Solution {
public:
    int lengthOfLastWord(string s) {
        int t=s.length();
        int count=0;
        while (t>0 && s[t - 1]==' ') {
            t--;
        }
        for(int i=t-1;i>=0;i--){
            if(s[i]==' '){
                return count;
            }
            count++;
            
        }
    return count;}
};