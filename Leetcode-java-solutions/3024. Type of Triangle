class Solution {
    public String triangleType(int[] nums){
       for(int i=0;i<nums.length-2;i++){
        if(nums[i]==nums[i+1] && nums[i]==nums[i+2]){
            return "equilateral";
        }
        else if((nums[i]==nums[i+1] && nums[i]!=nums[i+1]) || nums[i+1]==nums[i+2] && nums[i+1]!=nums[i]){
            return "isosceles";
        }
        else if(nums[i]!=nums[i+1] && nums[i+1]!=nums[i+2]){
            return "scalene";
        }
       }
    return "none";
    }
}