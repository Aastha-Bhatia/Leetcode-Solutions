class Solution{
    public List<String> wordSubsets(String[] words1, String[] words2){
        List<String> list=new ArrayList<>();
        for(int i=0;i<words1.length;i++){
            boolean found=true;
            for(int j=0;j<words2.length;j++){
                if(words1[i].contains(words2[j])){
                    continue;
                }
                else{
                    found=false;
                    break;
                }
            }
            if(found){
                list.add(words1[i]);
            }
        }
        return list;
    }
}