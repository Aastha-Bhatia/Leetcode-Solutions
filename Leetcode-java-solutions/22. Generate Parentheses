class Solution{
    public void generate(int n,String s,List<String> list,int opening,int closing){
        // base case.. 
        if(opening==n && closing==n){
            list.add(s);
            return;
        }
        // we have 2 possibilies- adding "(" or ")"
        if(opening<n){
            generate(n,s+"(",list,opening+1,closing);
        }
        if(closing<opening && closing<n){
            generate(n,s+")",list,opening,closing+1);
        }
    }
    public List<String> generateParenthesis(int n){
        List<String> list=new ArrayList<>();
        String s="";
        generate(n,s,list,0,0);
        return list;
    }
}