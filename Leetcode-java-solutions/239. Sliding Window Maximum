class Solution{
    public int[] maxSlidingWindow(int[] nums, int k){
        Deque<Integer> dq=new LinkedList<>();
        int n=nums.length;
        int ans[]=new int[n-k+1];
        int x=0;
        for(int i=0;i<n;i++){
            if(dq.size()!=0 && dq.peek()<=i-k){
                dq.poll();
            }
            while(!dq.isEmpty() && nums[i]>=nums[dq.peekLast()]){
                dq.pollLast();
            }
            dq.add(i);
            if(i+1-k>=0){
                // window mil gayi
                ans[x]=nums[dq.peek()];
                x++;
            }
        }
        return ans;
    }
}