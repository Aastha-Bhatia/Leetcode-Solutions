class Solution {
    public int maxProduct(int[] nums){
        int max=0;
        for(int i=0;i<nums.length;i++){
            for(int j=i+1;j<nums.length;j++){
                int prod=1;
                for(int k=i;k<=j;k++){
                    prod=prod*nums[k];
                    max=Math.max(prod,max);
                }
            }
        }
        return max;
    }
}