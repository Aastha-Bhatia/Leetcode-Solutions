class Solution{
    public String[] findRelativeRanks(int[] score){
        // 10 3 8 9 4
        int[] temp=Arrays.copyOf(score,score.length);
        Arrays.sort(temp);
        // 3 4 8 9 10 
        for(int i=0;i<score.length;i++){
            for(int j=temp.length-1;j>=0;j--){
                if(score[i]==temp[j]){
                    score[i]=temp.length-j;
                    break;
                }
            }
        }
        // 1 5 3 2 4 
        String ans[]=new String[score.length];
        for(int i=0;i<score.length;i++){
            if(score[i]==1){
                ans[i]="Gold Medal";
            }
            else if(score[i]==2){
                ans[i]="Silver Medal";
            }
            else if(score[i]==3){
                ans[i]="Bronze Medal";
            }
            else{
                ans[i]=""+score[i];
            }
        }
        return ans;
    }
}