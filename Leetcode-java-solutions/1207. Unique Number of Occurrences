class Solution{
    public boolean uniqueOccurrences(int[] arr){
        HashMap<Integer,Integer> mpp=new HashMap<>();
        for(int i=0;i<arr.length;i++){
            mpp.put(arr[i],mpp.getOrDefault(arr[i],0)+1);
        }
        // 1-3 2-2 3-1 
        HashSet<Integer> set=new HashSet<>();
        for(int i:mpp.values()){
            if(set.contains(i)){
                return false;
            }
            else{
                set.add(i);
            }
        }
        return true;
    }
}